-- Chargement de la bibliothèque de notification
local NotificationLoad = loadstring(game:HttpGet('https://raw.githubusercontent.com/treeofplant/Notif/main/library.lua', true))()

if not NotificationLoad then
    error("Notification library failed to load.")
end

-- Services
local Players = game:GetService("Players")

-- Créer les éléments GUI
local ScreenGui = Instance.new("ScreenGui")
local MainFrame = Instance.new("Frame")

-- TextBox pour les entrées
local OverboardTextBox = Instance.new("TextBox") -- Renommé en Overboard
local CarTextBox = Instance.new("TextBox") -- Renommé en Car
local HouseTextBox = Instance.new("TextBox") -- Renommé en House

-- Boutons pour chaque action
local Button1 = Instance.new("TextButton") 
local Button2 = Instance.new("TextButton") 
local Button3 = Instance.new("TextButton") 

-- Toggle UI Button
local ToggleButton = Instance.new("TextButton") 

-- Fonction d'application des coins arrondis
local function applyCorners(instance, cornerRadius)
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, cornerRadius)
    corner.Parent = instance
end

-- Propriétés du GUI
ScreenGui.Name = "CustomGui"
ScreenGui.Parent = game.CoreGui

MainFrame.Name = "MainFrame"
MainFrame.Parent = ScreenGui
MainFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
MainFrame.Position = UDim2.new(0.5, -120, 0.5, -150) -- Centré
MainFrame.Size = UDim2.new(0, 240, 0, 250) -- Taille
applyCorners(MainFrame, 10)

-- TextBox Overboard
OverboardTextBox.Parent = MainFrame
OverboardTextBox.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
OverboardTextBox.Position = UDim2.new(0.1, 0, 0.1, 0) -- Positionné en haut à gauche
OverboardTextBox.Size = UDim2.new(0.6, 0, 0.2, 0) -- Taille allongée et plus petite
OverboardTextBox.Font = Enum.Font.SourceSans
OverboardTextBox.PlaceholderText = "Enter input for Overboard"
OverboardTextBox.Text = ""
OverboardTextBox.TextColor3 = Color3.fromRGB(255, 255, 255)
OverboardTextBox.TextSize = 12

-- Bouton 1 à droite de OverboardTextBox
Button1.Parent = MainFrame
Button1.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
Button1.Position = UDim2.new(0.72, 0, 0.1, 0) -- Positionné à droite de OverboardTextBox
Button1.Size = UDim2.new(0.25, 0, 0.2, 0) -- Taille
Button1.Font = Enum.Font.GothamBold
Button1.Text = "Button 1"
Button1.TextColor3 = Color3.fromRGB(255, 255, 255)
Button1.TextSize = 12
applyCorners(Button1, 8)

-- TextBox Car
CarTextBox.Parent = MainFrame
CarTextBox.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
CarTextBox.Position = UDim2.new(0.1, 0, 0.35, 0) -- Positionné au milieu
