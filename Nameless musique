CarTextBox.Font = Enum.Font.SourceSans
CarTextBox.PlaceholderText = "Enter input for Car"
CarTextBox.Text = ""
CarTextBox.TextColor3 = Color3.fromRGB(255, 255, 255)
CarTextBox.TextSize = 12

-- Bouton 2 à droite de CarTextBox
Button2.Parent = MainFrame
Button2.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
Button2.Position = UDim2.new(0.72, 0, 0.35, 0) -- Positionné à droite de CarTextBox
Button2.Size = UDim2.new(0.25, 0, 0.2, 0) -- Taille
Button2.Font = Enum.Font.GothamBold
Button2.Text = "Button 2"
Button2.TextColor3 = Color3.fromRGB(255, 255, 255)
Button2.TextSize = 12
applyCorners(Button2, 8)

-- TextBox House
HouseTextBox.Parent = MainFrame
HouseTextBox.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
HouseTextBox.Position = UDim2.new(0.1, 0, 0.6, 0) -- Positionné en bas
HouseTextBox.Size = UDim2.new(0.6, 0, 0.2, 0) -- Taille allongée et plus petite
HouseTextBox.Font = Enum.Font.SourceSans
HouseTextBox.PlaceholderText = "Enter input for House"
HouseTextBox.Text = ""
HouseTextBox.TextColor3 = Color3.fromRGB(255, 255, 255)
HouseTextBox.TextSize = 12

-- Bouton 3 à droite de HouseTextBox
Button3.Parent = MainFrame
Button3.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
Button3.Position = UDim2.new(0.72, 0, 0.6, 0) -- Positionné à droite de HouseTextBox
Button3.Size = UDim2.new(0.25, 0, 0.2, 0) -- Taille
Button3.Font = Enum.Font.GothamBold
Button3.Text = "Button 3"
Button3.TextColor3 = Color3.fromRGB(255, 255, 255)
Button3.TextSize = 12
applyCorners(Button3, 8)

-- Créer le bouton Toggle UI en haut de l'écran
ToggleButton.Parent = ScreenGui
ToggleButton.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
ToggleButton.Position = UDim2.new(0.5, -50, 0, -25) -- Positionné en haut au centre
ToggleButton.Size = UDim2.new(0, 100, 0, 30)
ToggleButton.Font = Enum.Font.GothamBold
ToggleButton.Text = "Toggle UI"
ToggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
ToggleButton.TextSize = 14
applyCorners(ToggleButton, 8)

-- Fonction pour créer une notification
local function createNotification(title, description, timeout)
    NotificationLoad:NewNotification({
        ["Mode"] = "Custom",
        ["Title"] = title,
        ["Description"] = description,
        ["Notification_Image"] = "rbxassetid://",
        ["Notification_Sound"] = "rbxassetid://6031098704",
        ["Timeout"] = timeout or 5,
        ["Audio"] = false 
    })
end

-- Action du bouton 1
Button1.MouseButton1Click:Connect(function()
    local musicId = OverboardTextBox.Text -- Obtenir l'ID de musique depuis TextBox
    if musicId == "" then
        createNotification("Error", "Please enter a valid music ID.", 5)
        return
    end

    -- Arguments à envoyer au serveur
    local args = {
        [1] = "PickingScooterMusicText",
        [2] = musicId
    }

    -- Envoie au serveur via FireServer
    game:GetService("ReplicatedStorage").RE:FindFirstChild("1NoMoto1rVehicle1s"):FireServer(unpack(args))

    -- Notification de réussite
    createNotification("Success", "Music ID sent successfully!", 5)

    -- Réinitialisation du TextBox après envoi
    OverboardTextBox.Text = ""
end)

-- Action du bouton 2
Button2.MouseButton1Click:Connect(function()
    local musicId = CarTextBox.Text -- Obtenir l'ID de musique depuis TextBox
    if musicId == "" then
        createNotification("Error", "Please enter a valid music ID.", 5)
        return
    end

    -- Arguments à envoyer au serveur
    local args = {
        [1] = "PickingCarMusicText",
        [2] = musicId -- Utiliser l'ID de musique saisi
    }

    -- Envoie au serveur via FireServer
    game:GetService("ReplicatedStorage").RE:FindFirstChild("1Player1sCa1r"):FireServer(unpack(args))

    -- Notification de réussite
    createNotification("Success", "Car Music ID sent successfully!", 5)

    -- Réinitialisation du TextBox après envoi
    CarTextBox.Text = ""
end)

-- Action du bouton 3
Button3.MouseButton1Click:Connect(function()
    local musicId = HouseTextBox.Text -- Obtenir l'ID de musique depuis TextBox
    if musicId == "" then
        createNotification("Error", "Please enter a valid music ID.", 5)
        return
    end

    -- Arguments à envoyer au serveur
    local args = {
        [1] = "PickHouseMusicText",
        [2] = musicId -- Utiliser l'ID de musique saisi
    }

    -- Envoie au serveur via FireServer
    game:GetService("ReplicatedStorage").RE:FindFirstChild("1Player1sHous1e"):FireServer(unpack(args))

    -- Notification de réussite
    createNotification("Success", "House Music ID sent successfully!", 5)

    -- Réinitialisation du TextBox après envoi
    HouseTextBox.Text = ""
end)

-- Fonction pour toggler le GUI
local function toggleGUI()
    MainFrame.Visible = not MainFrame.Visible
end

-- Action du bouton Toggle UI
ToggleButton.MouseButton1Click:Connect(toggleGUI)

print("!!!!!!!!!!ONLY MUSUC PASS!!!!!!!!!!")
